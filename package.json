{
  "name": "@kubectl/actions",
  "version": "0.3.0",
  "description": "kubectl pxfd actions",
  "repository": "https://github.com/asdf-vm/actions",
  "author": "Tomas Hulata <thulata@pixelfederation.com>",
  "license": "MIT",
  "scripts": {
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.ts\" --cache",
